target C {
  platform: {
    name: "rp2040",
    board: "pololu_3pi_2040_robot"
  },
  single-threaded: true
}

import LED from "lib/LED.lf"



main reactor  {
  
  timer t(0, 400 ms);  
  state ledon: bool = false

  l = new LED();  

  reaction(t) -> l.set {=
  self->ledon =!self->ledon;
    lf_set(l.set,self->ledon); 
  =}
}


